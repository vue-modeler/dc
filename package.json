{
  "type": "module",
  "name": "@vue-modeler/dc",
  "version": "2.1.1-beta.1",
  "description": "Dependency container based on VUE effected scope and shared composable.",
  "author": "abratko",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
